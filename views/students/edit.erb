<form method="post" action="/students/<%= @students.id %>">
<label for="first-name">First Name:</label>
<input type="text" name="first_name" id="first-name" values="<%= @students.first_name %>">

<label for="last-name">Last Name:</label>
<input type="text" name="last_name" id="last-name" value="<%= @students.last_name %>">

<label for="house-select">Select a house:</label>
<select name="house_id" id="house_id"></select>
</form>


